    "#첫째 줄에 n(사람 수)\n",
    "#둘째 줄에 각 사람별 걸리는 시간\n",
    "n=int(input())\n",
    "a=list(map(int,input().split()))\n",
    "a.sort()\n",
    "temp=0\n",
    "for i in range(n):\n",
    "    temp+=a[i]\n",
    "    for j in range(n):\n",
    "        if i>j and i!=j:\n",
    "            temp+=a[j]\n",
    "        else:\n",
    "            pass\n",
    "print(temp)"
   ]
