   "source": [
    "n=list(input())\n",
    "li=[]\n",
    "dic={}\n",
    "for i in n:\n",
    "    temp=i.replace('6','9')  #6을 9로 대체\n",
    "    li.append(temp)\n",
    "\n",
    "for i in li:\n",
    "    if i=='9':\n",
    "        dic[i]=(li.count(i)+0.000005)/2  #round 내림 방지\n",
    "    else:\n",
    "        dic[i]=li.count(i)\n",
    "\n",
    "print(round(max(dic.values())))"
   ]
  }
 ],
