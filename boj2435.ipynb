    "n,k=map(int,input().split())\n",
    "weather=list(map(int,input().split()))\n",
    "\n",
    "comp=-float('inf')\n",
    "\n",
    "for i in range(n-k+1):\n",
    "    temp=0\n",
    "    for j in range(k):\n",
    "        temp+=weather[i+j]\n",
    "    comp=max(temp,comp)\n",
    "\n",
    "print(comp)"
   ]
