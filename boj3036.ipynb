   "source": [
    "n=int(input())\n",
    "a=list(map(int,input().split()))\n",
    "\n",
    "def gcd(a,b):\n",
    "    if b==0:\n",
    "        return a\n",
    "    else:\n",
    "        return gcd(b,a%b)\n",
    "    \n",
    "for i in range(1,n):\n",
    "    if a[0]%a[i]==0:\n",
    "        print(f'{a[0]//a[i]}/1')\n",
    "    else:\n",
    "        print(f'{a[0]//gcd(a[0],a[i])}/{a[i]//gcd(a[0],a[i])}')"
   ]
  }
